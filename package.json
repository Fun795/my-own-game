{
  "name": "my-own-game",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \\\"src/**/*.ts\\\" \\\"test/**/*.ts\\\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config test/jest-e2e.json",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js --config ./src/ormconfig",
    "typeorm:generate": "npm run build && npm run typeorm migration:generate -- -n",
    "typeorm:run": "npm run build && typeorm migration:run",
    "typeorm:revert": "npm run typeorm migration:revert"
  },
  "dependencies": {
    "@nestjs/common": "^8.2.3",
    "@nestjs/config": "^2.2.0",
    "@nestjs/core": "^8.2.3",
    "@nestjs/microservices": "^8.2.3",
    "@nestjs/platform-express": "^8.2.3",
    "@nestjs/swagger": "^6.0.4",
    "@nestjs/typeorm": "8.0.2",
    "amqp-connection-manager": "^4.1.3",
    "amqplib": "^0.10.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "dotenv": "^16.0.1",
    "nestjs-pino": "^3.1.1",
    "pg": "^8.7.3",
    "pino": "^8.3.1",
    "pino-http": "^8.2.0",
    "pino-pretty": "^8.1.0",
    "rascal": "^14.4.5",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.2.0",
    "typeorm": "0.2.45"
  },
  "devDependencies": {
    "@byndyusoft/eslint-config": "0.4.0",
    "@nestjs/cli": "^8.2.3",
    "@nestjs/schematics": "^8.0.11",
    "@nestjs/testing": "^8.2.3",
    "@types/express": "^4.17.13",
    "@types/jest": "28.1.4",
    "@types/node": "^16.0.0",
    "@types/rascal": "^10.0.6",
    "@types/supertest": "^2.0.11",
    "@types/validator": "^13.7.5",
    "@typescript-eslint/eslint-plugin": "^5.7.0",
    "@typescript-eslint/parser": "^5.7.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jest": "^25.3.0",
    "jest": "^27.4.5",
    "jest-extended": "^1.2.0",
    "prettier": "^2.3.2",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-jest": "28.0.5",
    "ts-loader": "^9.2.3",
    "jest-junit": "^13.0.0",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "4.0.0",
    "typescript": "^4.3.5"
  },
  "peerDependencies": {
    "eslint": "^7.23.0"
  },
  "jest": {
    "clearMocks": true,
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "testEnvironment": "node"
  }
}
